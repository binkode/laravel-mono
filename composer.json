{
    "name": "myckhel/laravel-mono",
    "description": "A description for laravel-mono.",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "myckhel",
            "email": "myckhel1@hotmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Myckhel\\Mono\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Myckhel\\Mono\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Myckhel\\Mono\\MonoServiceProvider"
            ],
            "aliases": {
                "Mono": "Myckhel\\Mono\\Facades\\Mono"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    }
}
